<Window x:Class="Oefening_2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Oefening_2"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <DockPanel>
            <Button x:Name="FirstButton"  Content="Button 1" DockPanel.Dock="Top" Padding="10"/>
            <Button x:Name="SecondButton"  Content="Button 2" DockPanel.Dock="Right" Padding="10" />
            <Button x:Name="ThirdButton"  Content="Button 3" DockPanel.Dock="Bottom" Padding="10" />
            <Button x:Name="FourthButton"  Content="Button 4" DockPanel.Dock="Left" Padding="10" />
            <Button x:Name="FifthButton"  Content="Button 5" DockPanel.Dock="Top" VerticalAlignment="top" Padding="10"/>
        </DockPanel>
    </Grid>
</Window>
